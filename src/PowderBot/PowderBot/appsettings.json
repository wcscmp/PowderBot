{
  "Logging": {
    "LogLevel": {
      "Default": "Error"
    },
    "LogFilePath": "Logs\\log-{Date}.txt"
  },
  "AllowedHosts": "*",
  "Storage": {
    "ConnectionString": "azure_table_connection_string",
    "Type": "Azure"
  },
  "Telegram": {
    "ApiKey": "telegram_api_key",
    "SecretToken": "telegram_secret_token"
  },
  "Scheduler": {
    "SecretToken": "scheduler_secret_token"
  }
  "ApplicationInsights": {
    "InstrumentationKey": "application_insights_instrumentation_key"
  }
}